{
  "name": "dttools-app",
  "version": "1.0.0",
  "description": "Design Thinking Tools - Complete Platform",
  "type": "module",
  "main": "server-simple.js",
  "scripts": {
    "start": "node server-simple.js",
    "db:push": "npx drizzle-kit push",
    "db:generate": "npx drizzle-kit generate",
    "db:create": "node --import tsx server/create-tables.ts",
    "dev": "NODE_ENV=development SESSION_SECRET=dev-secret-key-local-development-2025 PORT=5000 node --import tsx server/index.ts",
    "dev:compile": "tsc --project tsconfig.json && NODE_ENV=development SESSION_SECRET=dev-secret-key-local-development-2025 PORT=5000 node dist/server/index.js",
    "build": "node node_modules/vite/bin/vite.js build && cp -r dist/public server/public",
    "vercel-build": "echo 'Vercel build completed'",
    "replit": "npm install && npm run build && npm start"
  },
  "dependencies": {
